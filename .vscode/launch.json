{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9001,
            // Path to your source in container
            "serverSourceRoot": "/var/www/html",
            // Path to your local source
            "localSourceRoot": "/Users/yutianlin/Develop-Exercise/laravel/"
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        }
    ]
}